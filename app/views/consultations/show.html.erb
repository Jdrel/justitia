<div class="video-container">
<h4>Welcome to your consultation with <%= @consultation.lawyer.user.first_name + " " + @consultation.lawyer.user.last_name %>.</h4>
<h3><%= root_url %></h3>
</div>

<div class="video">
  <div id="remote-media" data-consultationid="<%= @consultation.id %>">
  </div>
  <div id="local-media" data-token="<%= @twilio_token %>" data-room="<%= @video_room %>">
  </div>
<div class="position-button">
<%= link_to "End videochat", end_videocall_path(@consultation), id: "disconnect", class: "btn btn-danger" %>
</div>
</div>
